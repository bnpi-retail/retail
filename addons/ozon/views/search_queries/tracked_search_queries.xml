<odoo>
  <data>

    <!-- Form -->
    <record id="view_action_tracked_search_queries_form" model="ir.ui.view">
        <field name="name">Поисковые запросы</field>
        <field name="model">ozon.tracked_search_queries</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <!-- Tree -->
    <record id="view_action_tracked_search_queries_tree" model="ir.ui.view">
        <field name="name">Поисковые запросы</field>
        <field name="model">ozon.tracked_search_queries</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>

    <!-- Action -->
    <record model="ir.actions.act_window" id="action_tracked_search_queries">
        <field name="name">Поисковые запросы</field>
        <field name="res_model">ozon.tracked_search_queries</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- Menu -->
    <menuitem name="Поисковые запросы"
        id="menu_search_queries_parser"
        parent="ozon.menu_issue"
        action="action_tracked_search_queries"
        sequence="30"/>

  </data>
</odoo>