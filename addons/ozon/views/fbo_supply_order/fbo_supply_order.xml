<odoo>
    <data>

        <!-- Form -->
        <record id="view_fbo_supply_order_form" model="ir.ui.view">
            <field name="name">Накладные по отгрузкам на склад FBO</field>
            <field name="model">ozon.fbo_supply_order</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="created_at" />
                        <field name="supply_date" />
                        <field name="supply_order_id" />
                        <field name="warehouse_id" />
                        <field name="total_items_count" />
                        <field name="total_quantity" />
                        <field name="skus" />
                        <field name="fbo_supply_order_products_ids">
                            <tree>
                                <field name="product_id" />
                                <field name="qty" />
                            </tree>
                        </field>
                    </group>
                </form>
            </field>
        </record>

        <!-- Tree -->
        <record id="view_fbo_supply_order_tree" model="ir.ui.view">
            <field name="name">Накладные по отгрузкам на склад FBO</field>
            <field name="model">ozon.fbo_supply_order</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="created_at" />
                    <field name="supply_date" />
                    <field name="supply_order_id" />
                    <field name="warehouse_id" />
                    <field name="total_items_count" />
                    <field name="total_quantity" />
                </tree>
            </field>
        </record>

        <!-- Action -->
        <record model="ir.actions.act_window" id="action_fbo_supply_order">
            <field name="name">Накладные по отгрузкам на склад FBO</field>
            <field name="res_model">ozon.fbo_supply_order</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!-- Menu -->
        <menuitem name="Накладные по отгрузкам на склад FBO"
            id="menu_fbo_supply_order"
            parent="ozon.menu_root"
            action="action_fbo_supply_order"
            sequence="39" />

    </data>
</odoo>