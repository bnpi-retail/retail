<odoo>
    <data>

        <!-- Form -->
        <record id="view_fbo_supply_order_product_form" model="ir.ui.view">
            <field name="name">Товар Ozon в поставке на склад Ozon (FBO)</field>
            <field name="model">ozon.fbo_supply_order_product</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="product_id" />
                        <field name="qty" />
                        <field name="fbo_supply_order_id" />
                        <field name="created_at" />
                        <field name="supply_date" />
                        <field name="warehouse_id" />
                    </group>
                </form>
            </field>
        </record>

        <!-- Tree -->
        <record id="view_fbo_supply_order_product_tree" model="ir.ui.view">
            <field name="name">Товар Ozon в поставке на склад Ozon (FBO)</field>
            <field name="model">ozon.fbo_supply_order_product</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="created_at" />
                    <field name="supply_date" />
                    <field name="fbo_supply_order_id" />
                    <field name="warehouse_id" />
                    <field name="qty" />
                </tree>
            </field>
        </record>

        <!-- Action -->
        <record model="ir.actions.act_window" id="action_fbo_supply_order_product">
            <field name="name">Товар Ozon в поставке</field>
            <field name="res_model">ozon.fbo_supply_order_product</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>