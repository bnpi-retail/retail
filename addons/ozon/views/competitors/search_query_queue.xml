<odoo>
  <data>

    <!-- Form -->

    <!-- Tree -->
    <record id="view_search_query_queue_tree" model="ir.ui.view">
        <field name="name">Очередь поисковых запросов</field>
        <field name="model">ozon.search_query_queue</field>
        <field name="arch" type="xml">
            <tree>
                <field name="query"/>
                <field name="status"/>
            </tree>
        </field>
    </record>

    <!-- Action -->
    <record model="ir.actions.act_window" id="action_search_query_queue">
        <field name="name">Очередь поисковых запросов</field>
        <field name="res_model">ozon.search_query_queue</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- Menu -->
    <menuitem name="Очередь поисковых запросов"
        id="menu_search_query_queue"
        parent="ozon.menu_competitors"
        action="action_search_query_queue"
        sequence="100"/>

  </data>
</odoo>